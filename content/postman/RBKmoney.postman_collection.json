		{
			"name": "createInvoice",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"var jsonData = JSON.parse(responseBody);",
							"postman.setEnvironmentVariable(\"invoice_id\", jsonData.id);"
						]
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"type": "text/javascript",
						"exec": [
							"//Prefer relative invoice dueDate",
							"var Now = new Date();",
							"Now.setHours(Now.getHours() + 1);",
							"postman.setEnvironmentVariable('now_plus_1h',(Now).toISOString())"
						]
					}
				}
			],
			"request": {
				"url": "https://api.rbk.money/v1/processing/invoices",
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "Bearer {{api_key}}",
						"description": ""
					},
					{
						"key": "X-Request-ID",
						"value": "{{$timestamp}}",
						"description": ""
					},
					{
						"key": "Content-Type",
						"value": "application/json; charset=utf-8",
						"description": ""
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\"shopID\": 1,\n\"amount\": 100000,\n\"currency\": \"RUB\",\n\"metadata\": \n  { \n    \"order_id\": \"Postman order\"\n  },\n\"dueDate\": \"{{now_plus_1h}}\",\n\"product\": \"Rubber Duck\",\n\"description\": \"Postman description\"\n}"
				},
